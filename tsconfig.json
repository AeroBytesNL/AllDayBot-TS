{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@utils/*": ["./utils/*"],
      "@enums/*": ["./enums/*"],
      "@src/*": ["./src/*"],
      "@media/*": ["./src/media/*"]
    },
    "sourceMap": true,
    "module": "commonjs",
    "target": "es2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": [
    "modules",
    "utils",
    "src",
    "enums",
    "utils/typedoc-plugin-schedule.ts"
  ]
}
